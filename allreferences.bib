keywords = {Biodiversity,Biological,Conservation of Natural Resources,Ecosystem,Environment,Flowers,Geography,Meta-Analysis,Meta-Analysis as Topic,Models,Phylogeny,Plant Physiological Phenomena,Plants,Pollen,Pollen: physiology,Pollination,Software,Species Specificity,Theoretical,Trees},
mendeley-tags = {Meta-Analysis,Pollination},
month = {jan},
