@article{Shi2015,
abstract = {Random Forest (RF) is a widely used algorithm for classification of remotely sensed data. Most research on image classification with RF has shown it to be useful for producing accurate classifications of non-parametric and high-dimensional data. RF consistently out-performs traditional classifiers and has been shown to be comparable to or better than other statistical and machine learning classifiers. Through a case study in peatland classification using LiDAR-derived DEM, DSM and point cloud derivatives, we present results of an analysis of the effects of input data characteristics on RF classifications. Several characteristics of input imagery and training data are shown to have significant impacts on the results of both out-of-bag and independent classification accuracy. High-dimension datasets should be reduced so that only the uncorrelated important variables are used in classifications. Despite the fact that RF itself is an ensemble approach, iterative classifications are recommended in order to assess the stability of predicted class extents and can be used to examine gradient classes or classes where overlap exists. As many training and validation samples should be collected as possible and independent error assessments should be used in assessing the quality of the classification. A randomly distributed sample of training data should be used or a sample that allows for the class proportions of training data to be representative of those found in the true environment and one that allows for the lowest level of spatial autocorrelation within the independent training data samples. In environments such as peatlands where subtle gradients exist between classes, training samples and input data can have a significant effect on the quality of mapping and monitoring.},
author = {Millard, Koreen and Richardson, Murray C.},
doi = {10.3390/rs70x000x},
isbn = {2072-4292},
issn = {2072-4292},
journal = {Remote Sensing},
keywords = {LiDAR,peatland,random forest classification,training data sample selection},
number = {1-x manuscripts},
pages = {1--26},
title = {{On the importance of training data sample selection in RF classification: a case study in peatland ecosystem mapping}},
volume = {7},
year = {2015}
}
