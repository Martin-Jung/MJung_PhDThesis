isbn = {DOI: 10.1111/j.1523-1739.2011.01674.x},
issn = {1523-1739},
journal = {Conservation biology : the journal of the Society for Conservation Biology},
keywords = {Complexity,Fragmentation,Landcover heterogeneity,Landscape matrix,NDVI,Parks,Reserves},
month = {aug},
number = {4},
pages = {610--8},
pmid = {21488956},
title = {{Measuring protected-area isolation and correlations of isolation with land-use intensity and protection status.}},
url = {http://doi.wiley.com/10.1111/j.1523-1739.2011.01674.x http://www.ncbi.nlm.nih.gov/pubmed/21488956},
volume = {26},
