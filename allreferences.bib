isbn = {1002100992},
issn = {14329840},
journal = {Ecosystems},
keywords = {Biodiversity,Bioenergy,Climate change,Forestry,Fragmentation,Infrastructure,Land use,Land-use change,MSA,Meta-Analysis,Modelling,Nitrogen,Policy options,Prediction,Protected areas},
mendeley-tags = {Land use,Meta-Analysis,Modelling,Prediction},
