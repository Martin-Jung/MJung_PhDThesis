primaryClass = {1049-8001},
title = {{Mapping fire regimes across time and space: Understanding coarse and fine-scale fire patterns}},
url = {http://www.publish.csiro.au/paper/WF01032 http://www.publish.csiro.au/?paper=WF01032},
volume = {10},
