@article{KleinGoldewijk2016,
abstract = {This paper presents an update and expansion of the History Database of the Global Environment (HYDE, v 3.2.000). HYDE is and internally consistent combination of updated historical population estimates and enhanced allocation algorithms with weighting maps for land use which are time-dependent. Categories include cropland, with a new distinction into irrigated and rain fed crops (other than rice) and irrigated and rain fed rice. Also grazing lands are provided, divided into more intensively used pasture and less intensively used rangeland. Population is represented by maps of total, urban, rural population and population density as well as built-up area. The period covered is 10{\&}thinsp;000{\&}thinsp;BCE to 2015{\&}thinsp;CE. We estimate that global population increased from 4.4 million people in 10{\&}thinsp;000 BCE to 7310{\&}thinsp;million in 2015{\&}thinsp;CE, resulting in a global population density increase of {\&}minus;2 to almost 56{\&}thinsp;cap{\&}thinsp;km{\&}minus;2 respectively. The urban built-up area evolved from almost zero to roughly 450{\&}thinsp;Mha at present, still only less than 0.5{\&}thinsp;{\%} of the total land surface of the globe. Cropland occupied approximately less than 1{\&}thinsp;{\%} of the global land area for a long time period until 1000{\&}thinsp;CE, quite similar like the grazing land area. In the next centuries the share of global cropland slowly grew to 2.3{\&}thinsp;{\%} in 1700{\&}thinsp;CE (ca. 294{\&}thinsp;Mha), 4.5{\&}thinsp;{\%} in 1850{\&}thinsp;CE (580{\&}thinsp;Mha) and 11{\&}thinsp;{\%} in 2015{\&}thinsp;CE (ca. 1564{\&}thinsp;Mha). Cropland can be further divided into rain-fed and irrigated land, and these categories can be further divided into rice and non-rice. Rainfed croplands are much more common with 2.2{\&}thinsp;{\%} in 1700{\&}thinsp;CE (294{\&}thinsp;Mha), 4.2{\&}thinsp;{\%} (552{\&}thinsp;Mha) in 1850{\&}thinsp;CE and 10{\&}thinsp;{\%} (1298{\&}thinsp;Mha) in 2015 CE, while irrigated croplands used less than 0.05{\&}thinsp;{\%} (5{\&}thinsp;Mha), 0.2{\&}thinsp;{\%} (28{\&}thinsp;Mha) and 2.0{\&}thinsp;{\%} in 2015{\&}thinsp;CE (266{\&}thinsp;Mha) resp. We estimate the irrigated rice area (paddy) at 0.02{\&}thinsp;{\%} in 1700{\&}thinsp;CE (2.7{\&}thinsp;Mha), 0.1{\&}thinsp;{\%} (127{\&}thinsp;Mha) in 1850{\&}thinsp;CE and 0.6{\&}thinsp;{\%} in 2015{\&}thinsp;CE (73{\&}thinsp;Mha). The share of grazing land grew from 5.1{\&}thinsp;{\%} in 1700{\&}thinsp;CE (668{\&}thinsp;Mha) to 9.2{\&}thinsp;{\%} in 1850{\&}thinsp;CE (1200{\&}thinsp;Mha) and 25.2{\&}thinsp;{\%} in 2015{\&}thinsp;CE (3284{\&}thinsp;Mha). Land used for grazing can be divided into more intensively used pasture and less (or not) managed rangeland. Pasture occupied 1.1{\&}thinsp;{\%} in 1700{\&}thinsp;CE (149{\&}thinsp;Mha), 2.0{\&}thinsp;{\%} in 1850{\&}thinsp;CE (257{\&}thinsp;Mha) and 6.1{\&}thinsp;{\%} (800{\&}thinsp;Mha) in 2015{\&}thinsp;CE, while rangelands usually occupy more space due to the occurrence in more arid regions and thus lower yields to sustain livestock. We estimate 4{\&}thinsp;{\%} in 1700{\&}thinsp;CE (519{\&}thinsp;Mha), 7.2{\&}thinsp;{\%} in 1850{\&}thinsp;CE (943{\&}thinsp;Mha) and 19{\&}thinsp;{\%} in 2015{\&}thinsp;CE (2483{\&}thinsp;Mha). All data can be downloaded from doi:10.17026/dans-znk-cfy3.},
author = {{Klein Goldewijk}, Kees and Beusen, Arthur and Doelman, Jonathan and Stehfest, Elke},
doi = {10.5194/essd-2016-58},
file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Klein Goldewijk et al. - 2016 - New anthropogenic land use estimates for the Holocene HYDE 3.2.pdf:pdf},
issn = {1866-3591},
journal = {Earth System Science Data Discussions},
month = {dec},
number = {December},
pages = {1--40},
title = {{New anthropogenic land use estimates for the Holocene; HYDE 3.2}},
url = {http://www.earth-syst-sci-data-discuss.net/essd-2016-58/},
year = {2016}
}
@article{Hurlbert1971,
abstract = {The recent literature on species diversity contains many semantic, conceptual, and technical problems. It is suggested that, as a result of these problems, species diversity has become a meaningless concept, that the term be abandoned, and that ecologists take a more critical approach to species?number relations and rely less on information theoretic and other analogies. As multispecific collections of organisms possess numerous statistical properties which conform to the conventional criteria for diversity indices, such collections are not intrinsically arrangeable in linear order along some diversity scale. Several such properties or "species composition parameters" having straightforward biological interpretations are presented as alternatives to the diversity approach. The two most basic of these are simply ?1=[n/n?1][1?$\Sigma$i(N{\_}i/{\_}N)2] =the proportion of potential interindividual encounters which is interspecific (as opposed to intraspecific), assuming every individual in the collection can encounter all other individuals, E(Sn) = $\Sigma$i [1?(N?Nin)/(Nn)] =the expected number of species in a sample of n individuals selected at random from a collection containing N individuals, S species, and Ni individuals in the ith species. See full-text article at JSTOR},
author = {Hurlbert, Stuart H},
doi = {10.2307/1934145},
file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hurlbert - 1971 - The Nonconcept of Species Diversity A Critique and Alternative Parameters.pdf:pdf},
isbn = {0012-9658},
issn = {00129658},
journal = {Ecology},
month = {jul},
number = {4},
pages = {577--586},
pmid = {1484},
title = {{The Nonconcept of Species Diversity: A Critique and Alternative Parameters}},
url = {http://www.jstor.org/stable/1934145 http://doi.wiley.com/10.2307/1934145},
volume = {52},
year = {1971}
}
@article{Hillebrand2017,
abstract = {Introduction: D-dimer assay, generally evaluated according to cutoff points calibrated for VTE exclusion, is used to estimate the individual risk of recurrence after a first idiopathic event of venous thromboembolism (VTE). Methods: Commercial D-dimer assays, evaluated according to predetermined cutoff levels for each assay, specific for age (lower in subjects {\textless}70 years) and gender (lower in males), were used in the recent DULCIS study. The present analysis compared the results obtained in the DULCIS with those that might have been had using the following different cutoff criteria: traditional cutoff for VTE exclusion, higher levels in subjects aged ≥60 years, or age multiplied by 10. Results: In young subjects, the DULCIS low levels resulted in half the recurrent events that would have occurred using the other criteria. In elderly patients, the DULCIS results were similar to those calculated for the two age-adjusted criteria. The adoption of traditional VTE exclusion criteria would have led to positive results in the large majority of elderly subjects, without a significant reduction in the rate of recurrent event. Conclusion: The results confirm the usefulness of the cutoff levels used in DULCIS.},
