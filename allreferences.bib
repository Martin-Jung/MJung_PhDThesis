journal = {Remote Sensing of Environment},
keywords = {Abundance,BBS,Biodiversity,Birds,Climate change,Heat waves,Land surface temperature,MODIS,Mixed effects models,North American Breeding Bird Survey,PRISM,Species richness,United States},
mendeley-tags = {BBS,MODIS},
month = {jan},
number = {1},
pages = {245--254},
publisher = {Elsevier Inc.},
title = {{Heat waves measured with MODIS land surface temperature data predict changes in avian community structure}},
url = {http://dx.doi.org/10.1016/j.rse.2010.08.024 http://linkinghub.elsevier.com/retrieve/pii/S0034425710002671},
