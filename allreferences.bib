mendeley-tags = {Traits},
month = {apr},
number = {2},
pages = {321--333},
title = {{Linking bird, carabid beetle and butterfly life-history traits to habitat fragmentation in mosaic landscapes}},
url = {http://doi.wiley.com/10.1111/j.1600-0587.2008.05546.x http://onlinelibrary.wiley.com/doi/10.1111/j.1600-0587.2008.05546.x/full},
volume = {32},
