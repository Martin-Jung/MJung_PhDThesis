file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fairbanks, McGwire - 2004 - Patterns of floristic richness in vegetation communities of California regional scale analysis with multi-te.pdf:pdf},
isbn = {0960-7447},
issn = {1466-822X},
journal = {Global Ecology and Biogeography},
keywords = {California,Environmental heterogeneity,Multiple-regression,Net primary productivity (NPP),Normalized difference vegetation index (NDVI),Plant species richness},
month = {may},
number = {3},
pages = {221--235},
title = {{Patterns of floristic richness in vegetation communities of California: regional scale analysis with multi-temporal NDVI}},
