journal = {ISPRS Journal of Photogrammetry and Remote Sensing},
keywords = {Change detection,Composite,Gomez,Land cover,Landsat,Large area,Mapping,Monitoring,Remote sensing,Sentinel 2},
mendeley-tags = {Gomez,Land cover},
month = {jun},
pages = {55--72},
title = {{Optical remotely sensed time series data for land cover classification: A review}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0924271616000769},
volume = {116},
year = {2016}
