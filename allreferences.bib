month = {mar},
number = {1938},
pages = {1010--1035},
pmid = {21282158},
title = {{Anthropogenic transformation of the terrestrial biosphere}},
url = {http://rsta.royalsocietypublishing.org/content/369/1938/1010.full.html{\#}{\%}5Cnhttp://rsta.royalsocietypublishing.org/content/369/1938/1010.ful{\%}5Cnhttp://rsta.royalsocietypublishing.org/letters/submit/roypta;369/ http://rsta.royalsocietypublishing.org/cgi/doi},
volume = {369},
