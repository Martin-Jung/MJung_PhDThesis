edition = {1},
file = {:C$\backslash$:/LocalStorage/References/1-s2.0-S0065250417300284-main.pdf:pdf},
isbn = {9780128139493},
issn = {00652504},
keywords = {Alpha diversity,Beta diversity,Biodiversity indicators,Biodiversity intactness index,Biodiversity models,Hockey-stick graph,Meta-analysis,PREDICTS,Representativeness},
mendeley-tags = {PREDICTS},
pages = {201--241},
