journal = {Journal of Applied Ecology},
keywords = {Agro-ecosystems,Apoidea,Conservation,Ecosystem services,Euglossini,Landscape ecology,Meliponini,Pollination,agro-ecosystems,conservation,ecosystem services,landscape  ecology,pollination},
month = {nov},
