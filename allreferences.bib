issn = {1469-185X},
journal = {Biological reviews of the Cambridge Philosophical Society},
keywords = {Animals,Biodiversity,Conservation of Natural Resources,Conservation of Natural Resources: methods,Ecosystem,Environmental Monitoring,Environmental Monitoring: methods,Hypotheses,Landscape,Plants,Review},
mendeley-tags = {Biodiversity,Hypotheses,Landscape,Review},
