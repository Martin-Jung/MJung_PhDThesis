isbn = {1365-2745},
issn = {00220477},
journal = {Journal of Ecology},
keywords = {Allometry,Biomass,Growth rate,Land-use history,Leaf area,Optimal partitioning,Southern Appalachians USA,Understorey},
month = {apr},
number = {3},
pages = {548--557},
title = {{Previous land use alters plant allocation and growth in forest herbs}},
