journal = {Oecologia},
keywords = {15th Century,16th Century,17th Century,18th Century,19th Century,20th Century,Adaptation,Czech Republic,Ecosystem,Environmental Monitoring,Europe,Flowers,Historical pattern,History,Invasion biology,Medieval,Paper,Physiological,Plants,Population Dynamics,Reproduction,Study,Survival,alien flora,history,immigration pattern,invasion,origin,species traits,year of introduction},
