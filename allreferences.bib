file = {:C$\backslash$:/LocalStorage/References/1999/Cowlishaw{\_}1999{\_}Predicting the Pattern of Decline of African Primate Diversity an Extinction Debt from Historical Deforestation.pdf:pdf},
issn = {0888-8892},
journal = {Conservation Biology},
keywords = {SAVE,africa,conservation,extinction debts,populations,primate},
mendeley-tags = {SAVE},
month = {oct},
number = {5},
pages = {1183--1193},
publisher = {Wiley Online Library},
title = {{Predicting the Pattern of Decline of African Primate Diversity: an Extinction Debt from Historical Deforestation}},
