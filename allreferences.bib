issn = {1540-9295},
journal = {Frontiers in Ecology and the Environment},
keywords = {Biodiversity,Biotic Homogenization,Homogenization,Study,biotic homogenization},
mendeley-tags = {Biotic Homogenization,Homogenization,Study},
