pages = {692--702},
publisher = {Elsevier Inc.},
title = {{Analysis of monotonic greening and browning trends from global NDVI time-series}},
url = {http://dx.doi.org/10.1016/j.rse.2010.10.011 http://linkinghub.elsevier.com/retrieve/pii/S0034425710003160},
