file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Huete et al. - 2002 - Overview of the radiometric and biophysical performance of the MODIS vegetation indices.pdf:pdf},
