journal = {Methods of Information in Medicine},
keywords = {Early stopping,Gradient boosting,Penalized regression,Resampling methods,Variable selection},
month = {feb},
number = {2},
pages = {178--186},
pmid = {22344292},
title = {{The Importance of Knowing When to Stop}},
url = {http://www.schattauer.de/index.php?id=1214{\&}doi=10.3414/ME11-02-0030},
volume = {51},
year = {2012}
