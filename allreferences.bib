issn = {1402-2001},
journal = {Applied Vegetation Science},
keywords = {cattle grazing,fire,mowing,plant species richness,sedge meadow},
number = {2},
pages = {307},
title = {{Biodiversity management of fens and fen meadows by grazing, cutting and burning}},
url = {http://doi.wiley.com/10.1658/1402-2001(2006)9[307:BMOFAF]2.0.CO;2},
volume = {9},
