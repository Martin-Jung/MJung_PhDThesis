issn = {0962-8452},
journal = {Proceedings of the Royal Society B: Biological Sciences},
month = {aug},
number = {1768},
pages = {20131201--20131201},
pmid = {23926147},
title = {{Environmental and historical imprints on beta diversity: insights from variation in rates of species turnover along gradients}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23926147 http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.2013.1201},
volume = {280},
year = {2013}
