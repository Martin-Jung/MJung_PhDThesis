file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lesiv et al. - 2018 - Characterizing the Spatial and Temporal Availability of Very High Resolution Satellite Imagery for Monitoring Appl.pdf:pdf},
issn = {1866-3591},
journal = {Earth System Science Data Discussions},
month = {mar},
