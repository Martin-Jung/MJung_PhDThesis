journal = {Oecologia},
keywords = {Animals,Bees,Bees: physiology,Body Size,Bombus spp,Ecosystem,Feeding Behavior,Feeding Behavior: physiology,Motor Activity,Motor Activity: physiology,Paper,Pollination,Wing,Wing: anatomy {\&} histology},
