isbn = {0962-1083},
issn = {09621083},
journal = {Molecular Ecology},
keywords = {fire,genetic diversity,local disturbance,microsatellites},
month = {jan},
number = {1},
pages = {56--66},
pmid = {21091560},
title = {{Fire increases variance in genetic characteristics of Florida Sand Skink (Plestiodon reynoldsi) local populations}},
