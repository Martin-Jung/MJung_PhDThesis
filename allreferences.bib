journal = {Global Ecology and Biogeography},
keywords = {2010 target,Biodiversity indicator,Biotic Homogenization,France,Paper,biotic homogenization,bird,breeding bird survey,community,landscape disturbance,landscape fragmentation,specialization},
mendeley-tags = {Biotic Homogenization,Paper},
month = {mar},
number = {2},
pages = {252--261},
title = {{Functional biotic homogenization of bird communities in disturbed landscapes}},
