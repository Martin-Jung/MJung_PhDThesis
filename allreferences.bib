journal = {Proceedings of the National Academy of Sciences},
keywords = {20th Century,21st Century,Biodiversity,Conservation of Natural Resources,Conservation of Natural Resources: statistics {\&} nu,Ecosystem,History,Plants,Plants: genetics},
month = {nov},
number = {48},
