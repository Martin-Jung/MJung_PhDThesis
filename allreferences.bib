isbn = {0006-3207},
issn = {00063207},
journal = {Biological Conservation},
keywords = {Anura,Atlantic Forest,Aves,Connectivity,Corridor,Extinction debt,Fragmentation,Mammalia,Secondary forest,South America,Temporal dynamics,abundance,biodiversity,biotic factor,connectivity,conservation planning,deforestation,dispersal,ecosystem response,extinction risk,habitat corridor,habitat fragmentation,landscape change,landscape history,old-growth forest,regeneration,secondary forest,species richness,temporal variation},
