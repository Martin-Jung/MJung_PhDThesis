journal = {Landscape Ecology},
keywords = {Ecosystem service,Forest,Habitat fragmentation,Habitat loss,Isolation,Open semi-natural habitat,Paper,Pollination,Seed-Dispersal,Swiss plateau},
mendeley-tags = {Isolation,Paper,Pollination,Seed-Dispersal},
