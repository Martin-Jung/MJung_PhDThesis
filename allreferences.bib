volume = {12},
year = {2006}
}
@article{Odum1969,
abstract = {The principle of ecological succession bear importantly on the relationships between man and nature. The framework of successional theory needs to be examined as a basis for resolving man's present environmental crisis. Most ideas pertaining to the development of ecological systems are based on descriptive data obtained by observing changes in biotic communities over long periods, or on highly theoretical assumptions; very few of the generally accepterd hypotheses have been tested experimentally. Some of the confusion, vagueness and lack of experimental work in this area stems from the tendency of ecologiusts to regard "succession" as a single straitforward idea; in actual fact, it entails an interacting complex of processes, some of which counteract one another.},
