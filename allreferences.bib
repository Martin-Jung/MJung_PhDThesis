}
@article{Goetz2014,
abstract = {Recent advances in remote sensing and ecological modeling warrant a timely and robust investigation$\backslash$n of the ecological variables that underlie large-scale patterns of breeding bird species richness,$\backslash$n particularly in the context of intensifying land use and climate change. Our objective was to$\backslash$n address this need using an array of bioclimatic and remotely sensed data sets representing$\backslash$n vegetation properties and structure, and other aspects of the physical environment. We first build$\backslash$n models of bird species richness across breeding bird survey (BBS) routes, and then spatially predict$\backslash$n richness across the coterminous US at moderately high spatial resolution (1 km). Predictor variables$\backslash$n were derived from various sources and maps of species richness were generated for four groups$\backslash$n (guilds) of birds with different breeding habitat affiliation (forest, grassland, open woodland,$\backslash$n scrub/shrub), as well as all guilds combined. Predictions of forest bird distributions were strong ($\backslash$n R 2 = 0.85), followed by grassland (0.76), scrub/shrub (0.63) and open woodland (0.60) species.$\backslash$n Vegetation properties were generally the strongest determinants of species richness, whereas$\backslash$n bioclimatic and lidar-derived vertical structure metrics were of variable importance and dependent$\backslash$n upon the guild type. Environmental variables (climate and the physical environment) were also$\backslash$n frequently selected predictors, but canopy structure variables were not as important as expected$\backslash$n based on more local to regional scale studies. Relatively sparse sampling of canopy structure$\backslash$n metrics from the satellite lidar sensor may have reduced their importance relative to other$\backslash$n predictor variables across the study domain. We discuss these results in the context of the$\backslash$n ecological drivers of species richness patterns, the spatial scale of bird diversity analyses, and$\backslash$n the potential of next generation space-borne lidar systems relevant to vegetation and ecosystem$\backslash$n studies. This study strengthens current understanding of bird species–climate–vegetation$\backslash$n relationships, which could be further advanced with improved canopy structure information across$\backslash$n spatial scales.},
author = {Goetz, Scott J and Sun, Mindy and Zolkos, Scott and Hansen, Andy and Dubayah, Ralph},
doi = {10.1088/1748-9326/9/3/034013},
file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Goetz et al. - 2014 - The relative importance of climate and vegetation properties on patterns of North American breeding bird species r.pdf:pdf},
isbn = {1748-9326},
issn = {1748-9326},
journal = {Environmental Research Letters},
month = {mar},
number = {3},
pages = {034013},
title = {{The relative importance of climate and vegetation properties on patterns of North American breeding bird species richness}},
url = {http://stacks.iop.org/1748-9326/9/i=3/a=034013 http://stacks.iop.org/1748-9326/9/i=3/a=034013?key=crossref.77b5b3cac49b48c03ff05dfdde265cf4},
volume = {9},
year = {2014}
}
@article{John2008,
abstract = {Changes in species composition and diversity are the inevitable consequences of climate change, as well as land use and land cover change. Predicting species richness at regional spatial scales using remotely sensed biophysical variables has emerged as a viable mechanism for monitoring species distribution. In this study, we evaluate the utility of MODIS-based productivity (GPP and EVI) and surface water content (NDSVI and LSWI) in predicting species richness in the semi-arid region of Inner Mongolia, China. We found that these metrics correlated well with plant species richness and could be used in biome- and life form-specific models. The relationships were evaluated on the basis of county-level data recorded from the Flora of Inner Mongolia, stratified by administrative (i.e., counties), biome boundaries (desert, grassland, and forest), and grouped by life forms (trees, grasses, bulbs, annuals and shrubs). The predictor variables included: the annual, mean, maximum, seasonal midpoint (EVImid), standard deviation of MODIS-derived GPP, EVI, LSWI and NDSVI. The regional pattern of species richness correlated with GPPSD (R2 = 0.27), which was also the best predictor for bulbs, perennial herbs and shrubs (R2 = 0.36, 0.29 and 0.40, respectively). The predictive power of models improved when counties with {\textgreater} 50{\%} of cropland were excluded from the analysis, where the seasonal dynamics of productivity and species richness deviate patterns in natural systems. When stratified by biome, GPPSD remained the best predictor of species richness in grasslands (R2 = 0.30), whereas the most variability was explained by NDSVImax in forests (R2 = 0.26), and LSWIavg in deserts (R2 = 0.61). The results demonstrated that biophysical estimates of productivity and water content can be used to predict plant species richness at the regional and biome levels. ?? 2008 Elsevier Inc. All rights reserved.},
author = {John, Ranjeet and Chen, Jiquan and Lu, Nan and Guo, Ke and Liang, Cunzhu and Wei, Yafen and Noormets, Asko and Ma, Keping and Han, Xingguo},
doi = {10.1016/j.rse.2007.09.013},
file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/John et al. - 2008 - Predicting plant diversity based on remote sensing products in the semi-arid region of Inner Mongolia.pdf:pdf},
isbn = {0034-4257},
issn = {00344257},
journal = {Remote Sensing of Environment},
keywords = {China,EVI,GPP,Inner Mongolia,LSWI,MODIS,NDSVI,Plant species richness,Semi-arid regions},
