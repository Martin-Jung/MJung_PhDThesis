journal = {Biological Conservation},
keywords = {Biodiversity index,BrayCurtis,Community composition,Deforestation,Habitat fragmentation,Habitat loss,Landscape pattern,Mapping,Turnover},
mendeley-tags = {BrayCurtis,Mapping,Turnover},
month = {dec},
