issn = {14391791},
journal = {Basic and Applied Ecology},
keywords = {Alien plant,Bee,Bees,Disturbance,Exotic plant,Human disturbance,Invasion biology,Invasive species,Paper,Preference,Resource use},
mendeley-tags = {Bee,Disturbance,Invasion biology,Paper},
month = {jun},
number = {4},
pages = {332--341},
publisher = {Elsevier GmbH},
title = {{Bees in disturbed habitats use, but do not prefer, alien plants}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S143917911000157X},
volume = {12},
year = {2011}
}
