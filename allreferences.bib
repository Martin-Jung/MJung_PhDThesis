issn = {1932-6203},
journal = {PloS one},
month = {jan},
number = {12},
pages = {e52288},
pmid = {23300634},
title = {{Identifying source populations and genetic structure for savannah elephants in human-dominated landscapes and protected areas in the Kenya-Tanzania borderlands.}},
