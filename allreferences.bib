isbn = {1365-2486},
issn = {1354-1013},
journal = {Global Change Biology},
keywords = {Biodiversity intactness index,Biodiversity loss,GroundTruthing,Land cover,Land degradation,Land use,NDVI,Remote-sensing,South Africa},
