pages = {1816--1825},
title = {{Landscape elements as potential barriers and corridors for bees, wasps and parasitoids}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S000632071100108X http://www.sciencedirect.com/science/article/pii/S000632071100108X},
volume = {144},
