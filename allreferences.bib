isbn = {0031-0182},
issn = {00310182},
journal = {Palaeogeography, Palaeoclimatology, Palaeoecology},
keywords = {Biostratigraphic marker,Ecological threshold,Human impact,Paludification,Radiocarbon date reversal,Soil erosion},
month = {jul},
pages = {49--61},
publisher = {Elsevier B.V.},
title = {{Human–environment interactions in an agricultural landscape: A 1400-yr sediment and pollen record from North Pare, NE Tanzania}},
