publisher = {Elsevier Ltd},
title = {{How Should Beta-Diversity Inform Biodiversity Conservation?}},
url = {http://dx.doi.org/10.1016/j.tree.2015.11.005 http://linkinghub.elsevier.com/retrieve/pii/S016953471500289X},
