pages = {e72348},
pmid = {24023616},
title = {{Beyond Precipitation: Physiographic Gradients Dictate the Relative Importance of Environmental Drivers on Savanna Vegetation}},
url = {http://dx.plos.org/10.1371/journal.pone.0072348},
volume = {8},
