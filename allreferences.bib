journal = {PloS one},
keywords = {Animal,Animals,Bee,Bees,Bees: physiology,Behavior,Dispersal,Ecosystem,Europe,Flight,Movement},
mendeley-tags = {Bee,Dispersal,Movement},
month = {jan},
number = {5},
pages = {e19997},
pmid = {21603569},
title = {{Space use of bumblebees (Bombus spp.) revealed by radio-tracking.}},
