file = {:C$\backslash$:/Users/mj291/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rocchini et al. - 2016 - Satellite remote sensing to monitor species diversity potential and pitfalls.pdf:pdf},
isbn = {20563485},
issn = {20563485},
journal = {Remote Sensing in Ecology and Conservation},
keywords = {alpha-diversity,beta-diversity,biodiversity,distance decay models,remote sensing},
month = {feb},
