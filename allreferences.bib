issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {21st Century,Ancient,Climate Change,Fires,Fires: history,Forecasting,History,Human Activities,Humans,Industry,Rain,Temperature},
month = {nov},
