journal = {Remote Sensing of Environment},
keywords = {BRDF,Enhanced vegetation index (EVI),Harvard forest,Howland forest,Morgan Monroe state forest,Normalized difference vegetation index (NDVI),Photochemical reflectance index (PRI)},
month = {dec},
