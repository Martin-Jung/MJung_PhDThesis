issn = {1365-2656},
journal = {The Journal of animal ecology},
keywords = {Anguilla,Anguilla: growth {\&} development,Anguilla: physiology,Animals,BRT,Biological,Boosted Regression Tree,Decision Trees,Ecology,Ecology: statistics {\&} numerical data,Method-Paper,Models,Regression Analysis,Statistical,Statistics as Topic},
