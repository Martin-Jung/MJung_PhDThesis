keywords = {Animals,Bees,Bees: physiology,Demography,Ecosystem,Fires,Flowers,Flowers: physiology,Forests,Nesting Behavior,Paper,Time Factors,Trees,bee abundance,bee diversity,fire,habitat specialists,indiana dunes,land use,nestedness,nesting resources,oak savanna,oligolectic,plant richness,species density},
