issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {Animals,Biodiversity,Biological,Climate,Ecosystem,Environment,Europe,Fungi,Fungi: growth {\&} development,Geography,Human Activities,Humans,Invertebrates,Invertebrates: growth {\&} development,Mammals,Mammals: growth {\&} development,Models,Plant Development,Population Density,Population Dynamics,Regression Analysis},
