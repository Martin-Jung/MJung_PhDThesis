isbn = {0034-4257},
issn = {00344257},
journal = {Remote Sensing of Environment},
keywords = {Bounding envelope,Eucalypt,Fast-growing plantations,Landsat,MOD13Q1,Mining time series data,Pattern recognition,Subsequence matching,Time series pattern analysis,Vegetation indices},
pages = {136--149},
publisher = {Elsevier Inc.},
title = {{Mapping short-rotation plantations at regional scale using MODIS time series: Case of eucalypt plantations in Brazil}},
