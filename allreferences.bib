issn = {01416707},
journal = {African Journal of Ecology},
keywords = {Africa,East African mountains,Forest disturbance by herbivores,Sinarundinaria alpina,Tropical montane forest,Vegetation zones},
mendeley-tags = {Africa},
month = {sep},
number = {3},
