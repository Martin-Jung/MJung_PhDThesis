issn = {00280836},
journal = {Nature},
month = {apr},
number = {6779},
pages = {755--758},
pmid = {10783887},
title = {{The metapopulation capacity of a fragmented landscape}},
url = {http://www.nature.com/doifinder/10.1038/35008063},
volume = {404},
year = {2000}
}
