keywords = {Age-Corellation,Apiformes,Bee,Paper,Stablity,conservation,habitat fragmentation,habitat heterogeneity,habitat quality,life-history traits,pollinators,secondary habitats,species–area relationships,succession},
mendeley-tags = {Age-Corellation,Bee,Paper,Stablity},
