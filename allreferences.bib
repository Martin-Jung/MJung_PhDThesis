issn = {1365-2656},
journal = {The Journal of animal ecology},
keywords = {Animal,Animal: physiology,Animals,Bee,Bees,Bees: physiology,Behavior,Biodiversity,Climate,Fires,Mediterranean Region,Switzerland,Traits},
mendeley-tags = {Bee,Traits},
month = {jan},
number = {1},
pages = {98--108},
pmid = {18705629},
title = {{Taxonomical vs. functional responses of bee communities to fire in two contrasting climatic regions.}},
